{
  "name": "alimranahmed/easyhttp",
  "type": "library",
  "keywords": [
    "http",
    "guzzle",
    "laravel"
  ],
  "description": "An easier Http client for laravel",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Al Imran Ahmed",
      "email": "al.imran.cse@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "AlImranAhmed\\EasyHttp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AlImranAhmed\\EasyHttp\\EasyHttpServiceProvider"
      ],
      "aliases": {
        "Http": "AlImranAhmed\\EasyHttp\\Facades\\Http"
      }
    }
  }
}
